main
var a, b;

{
    let a <- call InputNum();
    let b <- call InputNum();

    if a > b
    then
        call OutputNum(a - b);
        call OutputNewLine();

        call OutputNum(a / b);
        call OutputNewLine();
    else
        call OutputNum(a + b);
        call OutputNewLine();

        call OutputNum(a * b);
        call OutputNewLine();
    fi;
}.
